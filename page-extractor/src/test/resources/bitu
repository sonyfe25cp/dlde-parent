function convertAT() {
convertATnames = document.getElementsByName("convertATid");
if (convertATStatus == 'off') {
convertATStatus = 'on';
convertATsrc = "/images/standard/bb_at_on.png";
convertATalt = "引用@:开";
} else {
convertATStatus = 'off';
convertATsrc = "/images/standard/bb_at_off.png";
convertATalt = "引用@:关";
}
for (var i = 0; i < convertATnames.length; i++) {
convertATnames[i].src = convertATsrc;
convertATnames[i].alt = convertATalt;
}
}
</script>
